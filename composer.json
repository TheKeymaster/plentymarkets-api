{
    "name": "thekeymaster/plentymarkets-api",
    "description": "Library for PlentyMarkets REST API requests",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Brader",
            "email": "dominik@brader.co.at",
            "role": "Maintainer"
        }
    ],
    "require": {
        "plentymarkets/plugin-interface": "dev-beta7",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "PlentyMarkets\\Api\\": "src/PlentyMarkets/Api"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlentyMarkets\\Api\\Tests\\": "tests/PlentyMarkets/Api"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --coverage-text --coverage-clover=coverage.xml"
    }
}
